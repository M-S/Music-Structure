<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicstructure">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".NowPlayingActivity"
            android:label="@string/now_playing" />
        <activity
            android:name=".RecentActivityActivity"
            android:label="@string/recent_activity" />
        <activity
            android:name=".MusicLibraryActivity"
            android:label="@string/music_library" />
        <activity
            android:name=".PlayListsActivity"
            android:label="@string/playlists" />
        <activity android:name=".AlbumsActivity"
            android:label="@string/albums"/>
        <activity android:name=".ArtistsActivity"
            android:label="@string/artists"/>
        <activity android:name=".SongsActivity"
            android:label="@string/songs"></activity>
    </application>

</manifest>